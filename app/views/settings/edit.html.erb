<div class="set">
<br></br><br></br><br></br><br></br>
<%if flash.present?%>
    <div id="flash">
        <%= render partial: "shared/flash_messages", flash: flash %>
    </div>
  <% end %>
<%= form_for(@setting) do |f| %>
        <div class="field">
            <%= f.text_field :country, placeholder: "Country", class:"form-control" %>
        </div>
        <div class="field">
            <%= f.text_field :language, placeholder: "Language", class:"form-control" %>
        </div>    
         
        <div class="field">
            <%= f.submit "Guardar", class:"btn btn-sm btn-white white pull-right", style:"margin-top: 15px;" %>
        </div>
    <% end %>
</div>